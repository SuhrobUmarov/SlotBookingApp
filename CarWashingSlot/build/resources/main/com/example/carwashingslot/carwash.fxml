<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.carwashingslot.Controller">
    <padding>
        <javafx.geometry.Insets topRightBottomLeft="10" />
    </padding>
    <children>
        <Label text="Name:" />
        <TextField fx:id="nameTextField" />
        <Label text="Time Slot:" />
        <ComboBox fx:id="timeComboBox" />
        <Button text="Book" onAction="#bookSlot" />
    </children>
</VBox>
